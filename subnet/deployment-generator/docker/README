run from upper directory (under deployment-generator):
  docker build -t generator -f docker/Dockerfile --build-arg SUBNET_BRANCH=easy-genesis .
  docker run                                                           \
    --env-file docker.env                                              \
    -v $(pwd)/generated/deployment.json:/app/generated/deployment.json \
    --entrypoint 'bash'                                                \
    generator /app/docker/deploy_csc.sh